{
  "name": "react-bacon-isomorphic",
  "version": "0.0.1",
  "author": "Matti Lankinen <m.lankinen@iki.fi>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "bundle": "browserify app/client.js --extension .jsx -t babelify | uglifyjs --compress --mangle > public/bundle.js",
    "watch": "npm run watch:js & npm run watch:server & node_modules/.bin/livereactload monitor -n public/bundle.js & wait",
    "watch:server": "nodemon --watch app app.js",
    "watch:js": "watchify app/client.js -v --extension .jsx --debug -t babelify -t livereactload -o public/bundle.js"
  },
  "dependencies": {
    "babel": "5.2.15",
    "baconjs": "0.7.53",
    "body-parser": "^1.12.4",
    "express": "^4.12.3",
    "ramda": "^0.13.0",
    "react": "0.13.2",
    "serve-static": "1.9.3",
    "todomvc-app-css": "1.0.0",
    "todomvc-common": "1.0.1",
    "uglify-js": "^2.4.23",
    "whatwg-fetch": "^0.9.0"
  },
  "engines": {
    "node": "0.12.x"
  },
  "devDependencies": {
    "babelify": "6.0.2",
    "browserify": "10.1.0",
    "livereactload": "^0.5.2",
    "watchify": "3.2.1"
  }
}
